
<div class="wrap_post_index">

    <h1><%= title %></h1>

    <% posts.forEach(function(post){ %>

        <div class="post_card">

            <h2><a href="<%= post.href %>"><%= post.title %></a></h2>
			date:<%= formatDate(post.date) %>

        </div>

    <% }); %>

    <%- prevNextDiv() %>

</div>
